{
  "name": "graphql-friendly",
  "version": "0.0.6",
  "description": "An easy and friendly graphql client, alternative to apollo-client",
  "author": "Tchoupinax <corentinfiloche@hotmail.fr> (https://corentinfiloche.xyz)",
  "keywords": [
    "graphql-friendly",
    "graphql",
    "apollo",
    "client",
    "vue",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tchoupinax/graphql-friendly"
  },
  "bugs": {
    "url": "https://github.com/Tchoupinax/graphql-friendly/issues"
  },
  "license": "MIT",
  "scripts": {
    "test": "echo 0",
    "start": "klap start --no-minify",
    "watch": "klap watch --no-minify",
    "build": "klap build --no-minify"
  },
  "dependencies": {
    "axios": "1.6.2",
    "graphql": "16.8.1",
    "graphql-subscriptions-client": "0.16.4"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "klap": "7.0.11",
    "np": "9.2.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/graphql-friendly.js",
      "import": "./dist/graphql-friendly.js",
      "require": "./dist/graphql-friendly.cjs.js",
      "umd": "./dist/graphql-friendly.umd.js"
    },
    "./": "./",
    "./package.json": "./package.json"
  },
  "source": "src/index.ts",
  "main": "dist/graphql-friendly.cjs.js",
  "unpkg": "dist/graphql-friendly.js",
  "module": "dist/graphql-friendly.js",
  "browser": "dist/graphql-friendly.umd.js",
  "types": "dist/graphql-friendly.d.ts"
}
